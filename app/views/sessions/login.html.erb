<div class="top">
    <div class="top_wrapper">
        Ranks
        <%= button_to "Create New", root_path, method: :get %>
    </div>
</div>
<div class="wrapper">
  <div class="login_wrapper">
    <h1>Login</h1>
    <%= form_tag "/login" do %>
        <%= text_field_tag :name, nil, placeholder: "Event Name" %><br><br>
        <%= password_field_tag :password, nil, placeholder: "Password" %><br><br>
        <%= submit_tag "Log In" %>
    <% end %><br>
  </div>
</div>